{
  "version": "1.2.0",
  "configurations": [
    {
      "name": "vscode-formatting-toggle",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceRoot}"],
      "stopOnEntry": false,
      "sourceMaps": true,
      "outFiles": ["${workspaceRoot}/out/**/*.js"]
    }
  ]
}
